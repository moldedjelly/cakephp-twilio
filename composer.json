{
    "name": "moldedjelly/cakephp-twilio",
    "type": "cakephp-plugin",
    "version": "dev-master",
    "description": "Twilio CakePHP plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "moldedjelly",
            "homepage": "https://github.com/moldedjelly",
            "role": "Author"
        },
        {
            "name": "Mathew Johnson",
